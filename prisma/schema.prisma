generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model todos {
  id         Int      @id @default(autoincrement())
  text       String   @db.VarChar(200)
  done       Boolean
  created_at DateTime @default(now()) @db.DateTime(0)
}
